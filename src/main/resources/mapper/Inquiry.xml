<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.shop.repository.IncuiryRepository">
    <select id="findAll" resultType="InquiryDTO">
        SELECT
            *
        FROM
            inquiry
    </select>

    <select id="findById" resultType="InquiryDTO">
        SELECT
            *
        FROM
            inquiry
        WHERE
            id = #{id}
    </select>

</mapper>